{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "prebuild": {
      "inputs": [
        "next/**",
        "tailwind/**"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "build": {
      "dependsOn": [
        "lint",
        "prebuild",
        "^build"
      ],
      "outputs": [
        "dist/**",
        ".next/**"
      ]
    },
    "test": {
      "dependsOn": [
        "^build"
      ],
      "outputs": []
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "dependsOn": [
        "prebuild"
      ],
      "cache": false
    }
  }
}